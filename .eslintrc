{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0
  }
}
