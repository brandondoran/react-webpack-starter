{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "Minimal React Starter with Webpack",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-register",
    "test:watch": "npm test -- -w",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.prod.js --progress --profile --colors",
    "start": "node server"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "starter",
    "boilerplate",
    "hmr"
  ],
  "author": "Brandon Doran <bdoran@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/brandondoran/react-webpack-starter",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.0.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.14.0",
    "html-webpack-plugin": "^2.9.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "babel-register": "^6.4.3",
    "compression": "^1.6.1",
    "express": "^4.13.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ]
  }
}
