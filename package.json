{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "Minimal React Starter with Webpack",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-register",
    "test:watch": "npm test -- -w",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack/webpack.prod.babel.js --progress --profile --colors",
    "start": "node server"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "starter",
    "boilerplate",
    "hmr"
  ],
  "author": "Brandon Doran <bdoran@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/brandondoran/react-webpack-starter",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "compression-webpack-plugin": "^0.3.1",
    "cross-env": "^1.0.7",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.14.0",
    "html-webpack-plugin": "^2.20.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.1",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.4.3",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "babel-register": "^6.4.3",
    "compression": "^1.6.1",
    "express": "^4.13.3",
    "react": "^15.5.1",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.1"
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ]
  }
}
